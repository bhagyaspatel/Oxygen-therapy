        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
                android:text="@string/risk_of_type_2_failure"
                android:textColor="@color/text_black"/>

            <TextView
                android:id="@+id/isRisk"
                android:text="Yes/No"
                android:layout_width="match_parent"
                android:singleLine="false"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
                android:text="@string/target_saturation"
                android:textColor="@color/text_black"/>

            <TextView
                android:id="@+id/saturation"
                android:text="Yes/No"
                android:layout_width="match_parent"
                android:singleLine="false"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
                android:text="@string/baseline_parameters"
                android:textColor="@color/text_black"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15sdp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="HR-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/hr"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="BP-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/bp"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="SPO2-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/spo2"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="RR-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/rr"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
                android:text="@string/abg"
                android:textColor="@color/text_black"/>

            <LinearLayout
                android:layout_marginStart="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="PH-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/ph"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="PCO2-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/pco2"
                            android:textColor="@color/black"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="PO2-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/po2"
                            android:inputType="numberDecimal"
                            android:layout_width="@dimen/_60sdp"
                            android:textColor="@color/black"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_12sdp"
                            android:text="HCO3-"
                            android:textColor="@color/text_black"/>

                        <TextView
                            android:id="@+id/hco3"
                            android:layout_width="@dimen/_60sdp"
                            android:textColor="@color/black"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_height="wrap_content"
                            android:inputType="numberDecimal" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_12sdp"
                        android:text="LACTATE-"
                        android:textColor="@color/text_black"/>

                    <TextView
                        android:id="@+id/lactate"
                        android:layout_width="@dimen/_100sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:textColor="@color/black"
                        android:hint="If Available"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
                android:text="@string/delivery_system_recomended"
                android:textColor="@color/text_black"/>

            <TextView
                android:id="@+id/isSystem"
                android:text="Yes/No"
                android:layout_width="match_parent"
                android:singleLine="false"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/closeBtn"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:background="@color/lighter_app_green"
            android:layout_gravity="center"
            android:text="@string/close"
            android:outlineAmbientShadowColor="@color/darker_app_green"
            android:textColor="@color/darker_app_green"
            android:layout_marginTop="@dimen/_20sdp" />
    </LinearLayout>



</FrameLayout>